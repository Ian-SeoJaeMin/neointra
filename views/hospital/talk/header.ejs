<!doctype html>
<html lang="ko">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">    
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, user-scalable=no">
    <meta name="format-detection" content="telephone=yes">
    <title>
        <%= title.company_e %> - A/S 접수
    </title>

    <% include ../../includes/css %>
    <link rel="stylesheet" href="/css/talk.css">
    
    
    <!-- [if lt IE 9]>
        <script src="/components/html5shiv/html5shiv.min.js"></script>    
        <script src="/components/respond/respond.min.js"></script>
    <![endif]-->
    <script src="/js/polyfill.min.js"></script>
    <script>
        <% var notLoad = typeof service === 'object' ? 0 : 1 %>
        <% var _service = notLoad === 0 ? service : {}, _count = notLoad === 0 ? count : {} %>
        

        (function(){
            window.params = {
                service: '<%- JSON.stringify(_service) %>',
                count: '<%- JSON.stringify(_count) %>',
                hospital: '<%- JSON.stringify(hospital) %>',
                notLoad: '<%- notLoad %>'
            }
            window.params.service = JSON.parse(window.params.service.replace(/\r/gim, '\\r').replace(/\n/gim, '\\n'));                        
            window.params.count = JSON.parse(window.params.count);
            window.params.hospital = JSON.parse(window.params.hospital);
            window.params.notLoad = parseInt(window.params.notLoad);
        })();
    </script>
    
</head>

<body class="nav-md footer_fixed bg-white">