<script src="https://cdnjs.cloudflare.com/ajax/libs/jexcel/1.5.7/js/jquery.jexcel.js"></script>
<script src="/js/datepicker.js"></script>
<script>
    $(document).ready(function () {

        $('a[data-toggle="tab"]').on('shown.bs.tab', function (e) {
            // var target = $(e.target).attr("href") // activated tab
            // alert(target);

            var url = $(this).data('url');
            var target = $(e.target).attr("href")
            $(target).append(
                '<iframe class="iframe100" src="' + url + '" allowfullscreen></iframe>'
            )
        }).on('hidden.bs.tab', function (e) {
            console.log(e)
            var target = $(e.target).attr("href")
            $(target).empty()
        })

        // https: //docs.google.com/spreadsheets/d/1f1JNAkxubI0rpWYGGmonEutEl4M3-3j7_kHWBe5XRvc/edit?usp=sharing
        //     https: //docs.google.com/spreadsheets/d/17JyNtZv6_7sA5X9Qbg6BdYKeuS1Noecezit76Mbj6IQ/edit?usp=sharing
        //     https: //docs.google.com/spreadsheets/d/1VRYiibnjRyzSxx-VxVirNUt5guqXVFwnBaLGLFM1KUw/edit?usp=sharing

    })

</script>
