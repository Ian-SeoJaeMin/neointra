                        </tr>
                    </thead>
                    <tbody id="finder-button"></tbody>
                </table>
            </div>
            <div class="col-md-4 col-sm-12 col-xs-12">
                <table class="table table-hover table-linked jambo_table">
                    <thead>
                        <tr>
                            <th class="text-center">인덱스</th>
                            <th class="text-center">기관정보</th>
                            <th class="text-center">상태</th>
                            <th class="text-center">채택수</th>
                        </tr>
                    </thead>
                    <tbody id="finder-list"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>

