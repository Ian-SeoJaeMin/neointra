<% include ./header %>

<div class="container body">
    <div class="main_container">
        <% include ../includes/sidemenu.ejs %>
        <% include ../includes/topnav.ejs %>

        <div class="right_col" role="main">
            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h3> A/S 현황 </h3>
                            <div class="form-inline pull-right breakpoints-xs">
                                <%- include('../includes/datepicker.ejs', {mobile: false, label: false, name: "static", modal: false}) %>
                            </div>
                            <div class="clearfix"></div>
                            <div class="form-inline breakpoints-lg breakpoints-md breakpoints-sm">
                                <%- include('../includes/datepicker.ejs', {mobile: true, label: false, name: "static", modal: false}) %>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content table-responsive p-w-sm">
                            <table class="table table-bordered table-striped jambo_table neo-static">
                                <thead>
                                    <tr>
                                        <th style="width:100px;" class="text-center" rowspan="2">
                                            <i class="fa fa-users fa-2x"></i>
                                        </th>
                                        <th class="text-center" colspan="4">전체</th>
                                        <th class="text-center" colspan="6">Web 개별</th>
                                    </tr>
                                    <tr>
                                        <th style="width:100px;" class="text-center">합계
                                            <br>(Web + 전화)</th>
                                        <th class="text-center">Web</th>
                                        <th class="text-center">전화</th>
                                        <th class="text-center">방문</th>

                                        <th class="text-center">접수</th>
                                        <th class="text-center">공유</th>
                                        <th class="text-center">처리중</th>
                                        <th class="text-center">보류</th>
                                        <th class="text-center">완료</th>
                                        <th class="text-center">취소</th>

                                    </tr>
                                </thead>
                                <tbody id="service-status">
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h3>
                                <i class="fa fa-window-maximize"></i> 실행파일별 A/S 발생 </h3>
                            <div class="form-inline pull-right breakpoints-xs">
                                <%- include('../includes/datepicker.ejs', {mobile: false, label: false, name: "exe", modal: false}) %>
                            </div>

                            <div class="form-inline breakpoints-lg breakpoints-md breakpoints-sm">
                                <%- include('../includes/datepicker.ejs', {mobile: true, label: false, name: "exe", modal: false}) %>
                            </div>

                            <div class="form-group m-r-sm pull-right">
                                <div class="btn-group btn-group-sm" data-toggle="buttons">
                                    <label class="btn btn-success">
                                        <input type="checkbox" name="exe-static-area" /> 본사
                                    </label>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <!--div class="row" id="exe-chart">

                                </div-->
                            <div class="row">
                                <div class="col-md-12 col-sm-12 col-xs-12">
                                    <div class="table-responsive">
                                        <table class="table table-bordered neo-static neo-linked">
                                            <thead>
                                                <tr>
                                                    <th class="bg-navy">EMR</th>
                                                    <th class="bg-navy">처리구분</th>
                                                    <% Config.CONSTS.EXES.forEach(function(exe){ %>
                                                    <th class="bg-navy">
                                                        <%= exe %>
                                                    </th>
                                                    <% }) %>
                                                    <th class="bg-navy">합계</th>
                                                </tr>
                                            </thead>
                                            <tbody id="exe-table">
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h3>
                                <i class="fa fa-window-maximize"></i> 프로그램별 대분류/중분류 현황 </h3>

                            <div class="pull-right m-l-sm">
                                <select class="selectpicker category-options" id="program" title="프로그램" data-style="btn-sm btn-default"
                                    data-width="fit" data-size="5"></select>
                                <select class="selectpicker category-options" id="exe" title="실행파일" data-style="btn-sm btn-default"
                                    data-width="fit" data-size="5"></select>
                            </div>
                            <div class="form-inline pull-right breakpoints-xs">
                                <%- include('../includes/datepicker.ejs', {mobile: false, label: false, name: "categorys", modal: false}) %>
                            </div>

                            <div class="form-inline breakpoints-lg breakpoints-md breakpoints-sm">
                                <%- include('../includes/datepicker.ejs', {mobile: true, label: false, name: "categorys", modal: false}) %>
                            </div>



                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <div class="row">
                                <div class="col-md-4 col-sm-12 col-xs-12">
                                    <div class="chart-container">
                                        <canvas id="mainCategory-chart"></canvas>
                                    </div>
                                </div>
                                <div class="col-md-4 col-sm-12 col-xs-12">
                                    <div class="chart-container">
                                        <canvas id="subCategory-chart"></canvas>
                                    </div>
                                </div>
                                <div class="col-md-4 col-sm-12 col-xs-12">
                                    <div class="panel list-group" id="category-list">

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12">
                        <div class="x_panel">
                            <div class="x_title">
                                <h3>
                                    <i class="fa fa-window-maximize"></i> AS Finder 답변채택 현황 </h3>

                                <div class="pull-right m-l-sm">
                                    <select class="selectpicker finder-options" id="program" title="프로그램" data-style="btn-sm btn-default"
                                        data-width="fit" data-size="5">
                                        <option value="">전체</option>
                                        <option value="Eplus">Eplus</option>
                                        <option value="HanimacPro">HanimacPro</option>
                                        <option value="MediChart">MediChart</option>
                                        <option value="SENSE">SENSE</option>
                                        <option value="SENSE(한방)">SENSE(한방)</option>
                                    </select>
                                    <select class="selectpicker finder-options" id="exe" title="실행파일" data-style="btn-sm btn-default"
                                        data-width="fit" data-size="5">
                                        <option value="데스크">데스크</option>
                                        <option value="진료실">진료실</option>
                                        <option value="입원수납">입원수납</option>
                                        <option value="청구심사">청구심사</option>
                                        <option value="병동">병동</option>
                                        <option value="문서관리">문서관리</option>
                                        <option value="진료지원">진료지원</option>
                                        <option value="부가서비스">부가서비스</option>
                                        <option value="병원관리">병원관리</option>
                                        <option value="통계">통계</option>
                                        <option value="메인">메인</option>
                                        <option value="기타">기타</option>
                                    </select>
                                </div>
                                <div class="form-inline pull-right breakpoints-xs">
                                    <%- include('../includes/datepicker.ejs', {mobile: false, label: false, name: "finder", modal: false}) %>
                                </div>

                                <div class="form-inline breakpoints-lg breakpoints-md breakpoints-sm">
                                    <%- include('../includes/datepicker.ejs', {mobile: true, label: false, name: "finder", modal: false}) %>
                                </div>

                                <div class="clearfix"></div>
                            </div>
                            <div class="x_content">
                                <div class="row">
                                    <div class="col-md-4 col-sm-12 col-xs-12">
                                        <div class="chart-container">
                                            <canvas id="mainFinderCategory-chart"></canvas>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-12 col-xs-12">
                                        <div class="chart-container">
                                            <canvas id="subFinderCategory-chart"></canvas>
                                        </div>
                                    </div>
                                    <div class="col-md-4 col-sm-12 col-xs-12">
                                        <div class="panel list-group" id="finder-category-list">

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            <div class="row">
                <div class="col-md-6 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h3>
                                <i class="fa fa-bar-chart"></i>
                                지사별 A/S 발생
                            </h3>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <div class="chart-container">
                                <canvas id="area-chart"></canvas>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">

                            <h3>
                                <i class="fa fa-bar-chart"></i>
                                프로그램별 A/S 발생
                            </h3>
                            <div class="pull-right">
                                <input type="text" class="form-control daterange yearpicker" readonly id="yearpicker">
                            </div>
                            <div class="clearfix"></div>

                        </div>
                        <div class="x_content">
                            <div class="chart-container">
                                <canvas id="program-chart"></canvas>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

            <div class="row">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel">
                        <div class="x_title">
                            <h3>
                                시간대별 A/S 발생
                            </h3>
                            <div class="form-inline pull-right breakpoints-xs">
                                <div class="form-group datepicker-container">
                                    <div class="input-group input-group-sm">
                                        <div class="input-group-btn">
                                            <button class="btn btn-success btn-accept-day-range active" data-value="">전체</button>
                                            <button class="btn btn-success btn-accept-day-range" data-value="year"
                                                data-range="1">1년</button>
                                            <button class="btn btn-success btn-accept-day-range" data-value="months"
                                                data-range="6">6개월</button>
                                            <button class="btn btn-success btn-accept-day-range" data-value="months"
                                                data-range="3">3개월</button>
                                            <button class="btn btn-success btn-accept-day-range" data-value="months"
                                                data-range="1">1개월</button>
                                            <button class="btn <%= moment().format('dddd') === '월요일' ? 'btn-primary' : 'btn-default' %> btn-round btn-accept-day"
                                                data-day-name="월요일">월</button>
                                            <button class="btn <%= moment().format('dddd') === '화요일' ? 'btn-primary' : 'btn-default' %> btn-round btn-accept-day"
                                                data-day-name="화요일">화</button>
                                            <button class="btn <%= moment().format('dddd') === '수요일' ? 'btn-primary' : 'btn-default' %> btn-round btn-accept-day"
                                                data-day-name="수요일">수</button>
                                            <button class="btn <%= moment().format('dddd') === '목요일' ? 'btn-primary' : 'btn-default' %> btn-round btn-accept-day"
                                                data-day-name="목요일">목</button>
                                            <button class="btn <%= moment().format('dddd') === '금요일' ? 'btn-primary' : 'btn-default' %> btn-round btn-accept-day"
                                                data-day-name="금요일">금</button>
                                            <button class="btn <%= moment().format('dddd') === '토요일' ? 'btn-primary' : 'btn-default' %> btn-round btn-accept-day"
                                                data-day-name="토요일">토</button>
                                            <button class="btn <%= moment().format('dddd') === '일요일' ? 'btn-primary' : 'btn-default' %> btn-round btn-accept-day"
                                                data-day-name="일요일">일</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            <!-- <div class="chart-container"> -->
                            <canvas id="time-chart"></canvas>
                            <!-- </div> -->
                        </div>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>
<div class="modal fade exestatic-detail-modal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="vertical-container">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-body table-responsive">
                    <table class="table table-bordered ">
                        <thead>
                            <tr>
                                <th class="bg-navy">병원</th>
                                <th class="bg-navy">프로그램</th>
                                <th class="bg-navy">실행파일</th>
                                <th class="bg-navy">처리구분</th>
                                <th class="bg-navy">문의</th>
                                <th class="bg-navy">확인</th>
                                <th class="bg-navy">처리</th>
                                <!-- <th class="bg-navy">옵션/설정</th> -->
                                <!-- <th class="bg-navy">방법</th> -->
                            </tr>
                        </thead>
                    </table>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">닫기</button>
                    <!--button type="button" class="btn btn-primary"><i class="fa fa-print"></i></button-->
                </div>
            </div>
        </div>
    </div>
</div>
<% include ./footer %>
